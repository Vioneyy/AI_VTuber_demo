#############################
# Jeed AI VTuber - .env.example
# คัดลอกจาก .env โดยตัดค่า token/api key ออก
# เติมค่าที่จำเป็นก่อนรันระบบจริง
#############################

# ===== Discord =====
DISCORD_BOT_TOKEN=
ADMIN_USER_IDS=

# ===== OpenAI / LLM =====
OPENAI_API_KEY=
LLM_MODEL=gpt-4-turbo
LLM_MAX_TOKENS=150
LLM_TEMPERATURE=0.7
RESPONSE_TIMEOUT=10

# ===== TTS =====
TTS_ENGINE=f5_tts_thai
TTS_DEVICE=cuda
TTS_SPEED=0.95
TTS_STEPS=32
TTS_REFERENCE_WAV=reference_audio/jeed_voice.wav
F5_TTS_REF_AUDIO=reference_audio/jeed_voice.wav
F5_TTS_REF_TEXT=สวัสดีค่ะ

# ===== RVC =====
ENABLE_RVC=true
RVC_MODEL_PATH=rvc_models/jeed_anime.pth
RVC_INDEX_PATH=rvc_models/jeed_anime.index
RVC_PITCH=0
RVC_DEVICE=cuda
VOICE_PRESET=anime_girl

# ===== STT (Whisper) =====
WHISPER_MODEL=base
WHISPER_DEVICE=cuda
WHISPER_LANG=th

# Whisper.cpp (optional)
WHISPER_CPP_ENABLED=false
WHISPER_CPP_BIN_PATH=whisper.cpp/main.exe
WHISPER_CPP_MODEL_PATH=models/ggml-large-v3.bin
WHISPER_CPP_THREADS=4
WHISPER_CPP_NGL=35
WHISPER_CPP_TIMEOUT_MS=5000

# Python Whisper fallback
PY_WHISPER_MODEL=base
PY_WHISPER_FP16=false

# Discord voice processing
DISCORD_VOICE_STT_ENABLED=true
DISCORD_VOICE_RMS_THRESHOLD=350

# ===== VTube Studio =====
VTS_WS_URL=ws://localhost:8001
VTS_PLUGIN_NAME=Jeed AI VTuber
VTS_PLUGIN_DEVELOPER=AI VTuber Team
VTS_PLUGIN_TOKEN=
VTS_SEND_MIN_INTERVAL_MS=16

# ===== YouTube Live =====
YOUTUBE_ENABLED=false
YOUTUBE_VIDEO_ID=
YOUTUBE_STREAM_ID=

# ===== Personality =====
AI_NAME=จีด
AI_PERSONALITY=cute,energetic,friendly
AI_LANGUAGE=th

# ===== Performance =====
MAX_CONTEXT_MESSAGES=3
QUEUE_MAX_SIZE=50
AUDIO_SAMPLE_RATE=22050
AUDIO_CHUNK_SIZE=1024

# ===== Safety =====
SAFETY_FILTER_ENABLED=true
PROFANITY_FILTER_ENABLED=true